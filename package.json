  "private": true,
  "scripts": {
    "check-types": "tsc",
    "dev": "next dev",
    "build": "next build",
    "export": "next export",
    "deploy-part": "rm -rf node_modules/.cache && next build && next export",
    "deploy": "rm -rf node_modules/.cache && next build && next export && touch out/.nojekyll && git add out/ && git commit -m \"Deploy Next.js to gh-pages\" && git subtree push --prefix out origin gh-pages",
    "start": "next start",
    "eslint": "eslint",
    "lint": "(yarn run eslint src --ext .ts,.tsx) && (yarn run check-types)",
    "lint-fix": "eslint --fix --ext .ts,.tsx"
  },
  "dependencies": {
    "@tailwindcss/aspect-ratio": "^0.2.1",
