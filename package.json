    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-react": "^7.23.2",
    "postcss": "^8.1.10",
    "prettier": "^2.3.0",
    "tailwindcss": "^2.1.1",
    "typescript": "4.3.5"
  }
